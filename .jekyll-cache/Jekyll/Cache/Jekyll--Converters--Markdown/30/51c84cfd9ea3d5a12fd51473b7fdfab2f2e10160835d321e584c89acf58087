// handle jekyll site variables in data file
// (liquid variables in data files like csvs and yml are not processed)
function processSiteVars() {

	const config = {};
	
	/*
	config contains site variables from config.yml
	...only variables under site.vars are included
	*/
	
	config["piazza"] = `https://piazza.com/class/jzu453hfwul1qr`;
	
	config["email"] = `jversoza at cs dot nyu dot edu`;
	
	config["slides_img_prefix"] = `../../resources/img`;
	
	config["course_number"] = `CSCI-UA.0480`;
	
	config["course_section"] = `003`;
	
	config["course_name"] = `Data Management and Analysis`;
	
	config["course_semester"] = `Fall 2019`;
	
	config["course_time"] = `T and TH, 11:00-12:15PM`;
	
	config["course_room"] = `60 5th Ave, Room 110`;
	
	config["course_site"] = `https://cs.nyu.edu/courses/fall19/CSCI-UA.0480-003/`;
	
	config["course_site_alt"] = `http://cs.nyu.edu/~jversoza`;
	
	config["office_hours"] = `<br>Mon/Wed 11:25am - 12:25pm at WWH 422<br>Thurs 12:15pm - 1:15pm at 60 5th Ave lobby`;
	
	config["office_hours_room"] = `WWH 422 on Mon/Wed... Lobby, 60 5th Ave on Thurs`;
	
	config["midterm1_date"] = `N/A`;
	
	config["midterm1_time"] = `N/A`;
	
	config["final_date"] = `N/A`;
	
	config["final_time"] = `N/A`;
	
	config["final_room"] = `N/A`;
	
	config["book1"] = `Python for Data Analysis Book, 2nd Edition`;
	
	config["book1_link"] = `https://learning-oreilly-com.proxy.library.nyu.edu/library/view/python-for-data/9781491957653/`;
	
	config["book1_author"] = `Wes McKinney`;
	
	config["book2"] = `Practical SQL`;
	
	config["book2_link"] = `https://learning-oreilly-com.proxy.library.nyu.edu/library/view/practical-sql/9781492067580/`;
	
	config["book2_author"] = `Anthony DeBarros`;
	
	config["book3"] = `Think Stats`;
	
	config["book3_link"] = `http://greenteapress.com/thinkstats/`;
	
	config["book3_author"] = `Allen B. Downey`;
	
	config["book4"] = `Data Science from Scratch, 2nd Edition`;
	
	config["book4_link"] = `https://learning-oreilly-com.proxy.library.nyu.edu/library/view/data-science-from/9781492041122/`;
	
	config["book4_author"] = `Joel Grus`;
	
	config["book5"] = `Relational Database Design and Implementation, 3rd Edition`;
	
	config["book5_link"] = `https://learning-oreilly-com.proxy.library.nyu.edu/library/view/relational-database-design/9780123747303/`;
	
	config["book5_author"] = `Jan L. Harrington`;
	
	
	const re = /(site\.\w*)/g  
	const table = document.querySelector('table');

	console.log(table);
	const result = table.innerHTML.match(re);
	console.log(result);
	for(const s of result) {
		const k = s.replace('site.', '');
		
		table.innerHTML = table.innerHTML.replace(new RegExp('{{ ' + s + ' }}', 'g'), config[k]);
		
	}
	
	// console.log(config);
}

document.addEventListener('DOMContentLoaded', processSiteVars);
</script>

